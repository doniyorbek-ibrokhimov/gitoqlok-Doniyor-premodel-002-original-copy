///$tab OPTIMIZING TABLES
INCLUDE:
NOCONCATENATE LOAD * INLINE [
	RLDNR, PARVW, AWREF_REV
	'0L', 'AG', ' '
];

NOT_INCLUDE:
NOCONCATENATE LOAD * INLINE [
	KUNNR
	' '
];

INCLUDE_NOT_INCLUDE:
NOCONCATENATE LOAD
	KUNNR
FROM [lib://Data/!QVD DATA/QVD/SAP/ACDOCA.qvd](qvd)
WHERE NOT Exists(KUNNR)
;

DROP TABLE NOT_INCLUDE;